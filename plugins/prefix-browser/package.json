    "esprima": "^2.7.1",
    "in-publish": "^2.0.0",
    "lodash.property": "^4.1.2",
    "nodegit": "^0.8.0",
    "recast": "^0.11.2",
    "rimraf": "^2.5.0",
    "rollup": "^0.41.1",
    "uglify-js": "^2.6.2"
